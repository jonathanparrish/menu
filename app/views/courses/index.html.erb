

<div class="inner-div">
<p id="notice"><%= notice %></p>
<p>&nbsp;</p>
<p>&nbsp;</p>
<h2>Course</h2>

<table>
  <thead>
    <tr>
      <th>Course Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name%> </td>
        <td><%= link_to '<button type="button">Edit</button>' .html_safe, edit_course_path(course) %></td>
        <td><%= link_to '<button type="button">Destroy</button>' .html_safe, course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

</div>
